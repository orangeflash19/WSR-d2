<!--video area for admin and witness-->
<div id="videoPage" class="mastHead container">
  <div class="video-page-holder video-page-holder-horizontal row">
    <div class="left col">
      <div class="video-wrap">
        <div class="main-video">
          <div id="peerDiv" class="embed-responsive embed-responsive-16by9">

            <!-- <video id="mainVideoTag" class="embed-responsive-item" autoplay playsinline></video> -->
          </div>

        </div>
        <div class="small-video embed-responsive embed-responsive-16by9">
          <video id="smallVideoTag" class="embed-responsive-item" autoplay muted="mute"></video>
        </div>
      </div>

      <p>
        <button id="btnStart" class="btn btn-warning m-1">START RECORDING</button>
        <button id="btnStop" class="btn btn-warning m-1">STOP RECORDING</button>
        <button id="download" class="btn btn-warning m-1">DOWNLOAD</button>
        <button id="downloadtxt" class="btn btn-warning m-1">DOWNLOAD TEXT</button></p>
      <textarea name="" class="output" cols="30" rows="10" style="display: block;"></textarea>
      <div class="text-danger">
        <span id="errorMsg"></span>
      </div>
    </div>
  </div>
</div>
<!-- bundle.js for video call part -->
<script src="/socket.io/socket.io.js"></script>
<script src="/js/bundle.js" type="text/javascript"></script>
